<% @data['ntlm'].each do |login| %>
  <% user = login.public.present ? login.public.username : Metasploit::Credential::Exporter::Pwdump::BLANK_CRED_STRING %>
  <% pass = login.private.data.present ? login.private.data : Metasploit::Credential::Exporter::Pwdump::BLANK_CRED_STRING %>
  <%=	 user %>:<%= login.id %>:<%= pass %>

<% end %>